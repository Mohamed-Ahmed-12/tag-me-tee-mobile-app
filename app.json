{
  "expo": {
    "scheme": "tagmetee",
    "name": "QRCodeApp",
    "slug": "QRCodeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-black.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-black.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.qrcodeapp",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "43dc2ecf-4a89-4ea3-866c-2713884e2e3e"
      }
    }
  }
}
